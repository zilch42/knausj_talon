tag: user.multiple_cursors
-
(cursor|curse) multiple: user.multi_cursor_enable()
(cursor|curse) stop: user.multi_cursor_disable()
(cursor|curse) (up|source|north): user.multi_cursor_add_above()
(cursor|curse) (down|dunce|south): user.multi_cursor_add_below()
(cursor|curse) less: user.multi_cursor_select_fewer_occurrences()
(cursor|curse) more: user.multi_cursor_select_more_occurrences()
(cursor|curse) skip: user.multi_cursor_skip_occurrence()
(cursor|curse) all: user.multi_cursor_select_all_occurrences()
(cursor|curse) lines: user.multi_cursor_add_to_line_ends()
